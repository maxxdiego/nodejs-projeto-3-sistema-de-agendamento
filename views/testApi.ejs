<script>
    const endpoint = 'http://localhost:8080/getcalendar'
    
    async function pegarDados() {
        const res = await fetch(endpoint)
        const dados = await res.json()
        return dados
    }

    document.addEventListener("DOMContentLoaded", async () => {
        let dados = []

        try {
        dados = await pegarDados()
        }catch(err) {
        console.log("Erro!")
        console.log(err)
        }
        
        dados.forEach(show => {
            document.write(`ID: ${show.id}`)
            document.write(`<br>TÃ­tulo: ${show.title}<br><br>`)
        })        
    })

</script>
</html>